{
    "name": "gapple/drupalreleasedate",
    "description": "Release date predictor for Drupal Core",
    "authors": [
        {
            "name": "Geoff Appleby",
            "email": "geoff@gapple.ca"
        }
    ],
    "autoload": {
        "psr-4": {"DrupalReleaseDate\\": "src/DrupalReleaseDate/"}
    },
    "autoload-dev": {
        "psr-4": {"DrupalReleaseDate\\Tests\\": "src/tests/DrupalReleaseDate/Tests/"}
    },
    "config": {
        "platform": {
            "php": "7.0.22"
        }
    },
    "require": {
        "php": "^7.0",
        "cilex/console-service-provider": "~1.0",
        "doctrine/dbal": "~2.2",
        "guzzlehttp/guzzle": "^6.0",
        "paragonie/random_compat": "^2.0",
        "silex/silex": "~1.0",
        "symfony/console": "~2.4",
        "symfony/dom-crawler": "~2.4",
        "symfony/css-selector": "~2.4",
        "twig/twig": "~2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^2.0"
    }
}
